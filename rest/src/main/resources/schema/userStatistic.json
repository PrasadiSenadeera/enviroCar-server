{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://envirocar.org/api/stable/schema/userstatistic.json#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "distance",
    "duration"
  ],
  "properties": {
    "distance": {
      "type": "number"
    },
    "duration": {
      "type": "number"
    },
    "userstatistic": {
      "below60kmh": {
        "distance": {
          "type": "number"
        },
        "duration": {
          "type": "number"
        }
      },
      "above130kmh": {
        "distance": {
          "type": "number"
        },
        "duration": {
          "type": "number"
        }
      },
      "NaN": {
        "distance": {
          "type": "number"
        },
        "duration": {
          "type": "number"
        }
      }
    },
    "trackSummaries": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string"
          },
          "startPosition": {
            "geometry": {
              "$ref": "https://envirocar.org/api/stable/schema/geometry.json#"
            }
          },
          "endPosition": {
            "geometry": {
              "$ref": "https://envirocar.org/api/stable/schema/geometry.json#"
            }
          }
        }
      }
    }
  }
}