{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://envirocar.org/api/stable/schema/terms-of-use.json#",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "termsOfUse"
  ],
  "properties": {
    "termsOfUse": {
      "type": "array",
      "minItems": 0,
      "items": {
        "type": "object",
        "required": [
          "id",
          "issuedDate"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "issuedDate": {
            "type": "string",
            "format": "date"
          }
        }
      }
    }
  }
}